<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/layout11"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:paddingLeft="16dp"
            android:paddingStart="16dp"
            android:paddingRight="16dp"
            android:paddingEnd="16dp"
            android:background="@drawable/layer_card_background">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/primary_light">

                <TextView
                    android:id="@+id/dbName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/diver_name"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/divername"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/dbName"
                    android:layout_below="@id/View0"
                    android:layout_marginLeft="16dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@id/dbName"
                    android:layout_toEndOf="@id/dbName"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/dbMeet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/dbName"
                    android:layout_marginTop="8dp"
                    android:text="@string/meet_name"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/meetname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/dbMeet"
                    android:layout_below="@id/divername"
                    android:layout_marginLeft="16dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/dbName"
                    android:layout_toEndOf="@+id/dbName"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
            
            </RelativeLayout>
        
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout12"
            android:layout_below="@id/layout11"
            android:layout_marginTop="8dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:paddingLeft="16dp"
            android:paddingStart="16dp"
            android:paddingRight="16dp"
            android:paddingEnd="16dp"
            android:background="@drawable/layer_card_background">
    
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/primary_light">
    
                <TextView
                    android:id="@+id/diveType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:text="@string/dive_type_name"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/diveTypeValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/diveType"
                    android:layout_toRightOf="@id/diveType"
                    android:layout_toEndOf="@id/diveType"
                    android:layout_alignLeft="@+id/divePositionValue"
                    android:layout_alignStart="@+id/divePositionValue"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/diveName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/diveTypeValue"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/dive_name"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/diveNameValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/diveTypeValue"
                    android:layout_alignBaseline="@id/diveName"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/diveName"
                    android:layout_toEndOf="@+id/diveName"
                    android:layout_alignLeft="@+id/divePositionValue"
                    android:layout_alignStart="@+id/divePositionValue"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:singleLine="false"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/divePosition"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/diveNameValue"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="10dp"
                    android:text="@string/dive_position_name"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/divePositionValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/diveNameValue"
                    android:layout_alignBaseline="@id/divePosition"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/divePosition"
                    android:layout_toEndOf="@+id/divePosition"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/divedd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/divePosition"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="10dp"
                    android:text="@string/dd"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
        
                <TextView
                    android:id="@+id/ddValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/diveNameValue"
                    android:layout_alignBaseline="@id/divedd"
                    android:layout_marginTop="8dp"
                    android:layout_toRightOf="@+id/divePosition"
                    android:layout_toEndOf="@+id/divePosition"
                    android:layout_alignLeft="@+id/divePositionValue"
                    android:layout_alignStart="@+id/divePositionValue"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
                
            </RelativeLayout>
        
        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout13"
            android:layout_below="@id/layout12"
            android:layout_marginTop="8dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="16dp"
            android:paddingBottom="16dp"
            android:paddingLeft="16dp"
            android:paddingStart="16dp"
            android:paddingRight="16dp"
            android:paddingEnd="16dp"
            android:background="@drawable/layer_card_background">
    
            <LinearLayout
                android:id="@+id/layouts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/primary_light">
    
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/primary_light"/>
    
                <TextView
                    android:id="@+id/total2view"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:text="@string/score_total"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
    
                <TextView
                    android:id="@+id/scoreTotal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="8dp"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:maxLength="6"
                    android:background="@color/primary_light"/>
    
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/primary_light"/>
    
                <TextView
                    android:id="@+id/failedView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="@string/failed"
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/secondary_text"
                    android:background="@color/primary_light"/>
    
                <TextView
                    android:id="@+id/failedText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="8dp"
                    android:text=""
                    android:textSize="@dimen/text_size_medium"
                    android:textColor="@color/primary_text"
                    android:background="@color/primary_light"/>
    
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"
                    android:background="@color/primary_light"/>
    
            </LinearLayout>
    
            <View
                android:id="@+id/View3"
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="2dp"
                android:background="@color/primary"/>
    
            <TableLayout
                android:id="@+id/tableLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@color/primary_light">
    
                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:layout_marginTop="8dp"
                    android:background="@color/primary_light">
    
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>
    
                    <TextView
                        android:id="@+id/score1view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_1"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:background="@color/primary_light"/>
    
                    <TextView
                        android:id="@+id/score1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:background="@color/primary_light"/>
    
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>
    
                    <TextView
                        android:id="@+id/score2view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_2"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:background="@color/primary_light"/>
    
                    <TextView
                        android:id="@+id/score2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:background="@color/primary_light"/>
    
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>
                    <TextView
                        android:id="@+id/score3view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_3"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:background="@color/primary_light"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

    
                </TableRow>
    
                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:layout_marginTop="8dp"
                    android:background="@color/primary_light">
    
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score4view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_4"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>
    
                    <TextView
                        android:id="@+id/score4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>
    
                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score5view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_5"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score6view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_6"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>
    
                </TableRow>
    
                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dip"
                    android:layout_marginTop="8dp"
                    android:background="@color/primary_light">

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score7view"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/judge_7"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/score7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/fake1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:text="@string/failed"
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/secondary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <TextView
                        android:id="@+id/fake2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="@dimen/text_size_medium"
                        android:textColor="@color/primary_text"
                        android:visibility="invisible"
                        android:background="@color/primary_light"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_weight="1"
                        android:background="@color/primary_light"/>

                </TableRow>

            </TableLayout>

        </LinearLayout>

    </RelativeLayout>

</ScrollView>
